{
    "name": "shahkochaki/laravel-vault",
    "description": "Laravel Vault provides a lightweight, production-minded integration between Laravel and HashiCorp Vault. It supports KV v2, token-file authentication (Vault Agent / mounted tokens), configurable caching, and safe runtime config injection so you can fetch secrets at boot or on demand without committing secrets to source control.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "shahkochaki",
            "email": "ali.shahkochaki7@gmail.com"
        }
    ],
    "homepage": "https://github.com/shahkochaki/laravel-vault",
    "keywords": [
        "laravel",
        "vault",
        "hashicorp",
        "secrets",
        "keystore",
        "kv2"
    ],
    "support": {
        "issues": "https://github.com/shahkochaki/laravel-vault/issues",
        "source": "https://github.com/shahkochaki/laravel-vault"
    },
    "version": "1.2.2",
    "require": {
        "php": "^8.0",
        "illuminate/support": "^9.0|^10.0|^11.0|^12.0",
        "guzzlehttp/guzzle": "^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0 || ^10.0",
        "orchestra/testbench": "^6.0 || ^7.0 || ^8.0"
    },
    "autoload": {
        "psr-4": {
            "shahkochaki\\Vault\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "shahkochaki\\Vault\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "shahkochaki\\Vault\\VaultServiceProvider"
            ]
        }
    }
}